<div class="mainBody">
  <div class="leftBar">
    <div class="students">
      <div class="navBlock">
        <form method="GET" action="/student/new/{{id}}">
          <button id="addAStudent" class="navBlockButtons addAStudent">Add a Student</button>
        </form>
      </div>
      {{#each students}}
      <article id="{{this.id}}" class="student studentRow"><a style="color: rgb(122,95,164);"
          href="/student/{{this.teacherID}}/{{this._id}}">
          <div>{{this.name}}</div>
        </a>
        <div><img style="margin-left: 10px; cursor: pointer" id="deleteStud" width="15px" height="15px"
            src="data:image/png;base64,"
            alt="" srcset="">
        </div>
      </article>
      {{/each}}
    </div>
    <div class="oldPlans">
      <div class="navBlock">
        <form method="GET" action="/student/newPlan">
          <button id="addAPlan" class="navBlockButtons">Add a Plan</button>
        </form>
      </div>
      {{# each lessonPlans}}
      <article id="{{this.id}}" class="student studentRow">
        <div id="planForLesson" class="planString"><a style="color: rgb(122,95,164);"
            href="/student/update/{{this.id}}">{{this.theme}}</a>
        </div>
        <div>{{this.date}}</div>
        <div><img style="margin-left: 10px; cursor: pointer" id="deletePlan" width="15px" height="15px"
            src="data:image/png;base64,"
            alt="" srcset="">
        </div>
      </article>
      {{/each}}
    </div>
  </div>
  <div class="mainBlock">
    <form method="POST" id="{{studentInfo._id}}" action="/student/editStudent/{{studentInfo._id}}">
      <div class="infoBar">
        <div>Student: <input type="text" value="{{studentInfo.name}}" name="name" id=""></div>
        <div>E-mail: <input type="email" value="{{studentInfo.email}}" name="email"> </div>
        <div>Tel: <input type="text" name="phone" value=" {{studentInfo.phone}}" id=""></div>
        <div>Skype: <input value="{{studentInfo.skype}}" type="text" name="" id="skype"> </div>
      </div>
      <div class="timetableBar">
        <span>Lessons: </span>
        <div class="timeTableLesson"> Monday: <input name="Monday" value="{{studentInfo.Monday}}" type="time"></div>
        <div class="timeTableLesson"> Tuesday: <input name="Tuesday" value="{{studentInfo.Tuesday}}" type="time"></div>
        <div class="timeTableLesson"> Wednesday: <input name="Wednesday" value="{{studentInfo.Wednesday}}" type="time">
        </div>
        <div class="timeTableLesson"> Thursday: <input name="Thursday" value="{{studentInfo.Thursday}}" type="time">
        </div>
        <div class="timeTableLesson"> Friday: <input name="Friday" value="{{studentInfo.Friday}}" type="time"></div>
        <div class="timeTableLesson"> Saturday: <input name="Saturday" value="{{studentInfo.Saturday}}" type="time">
        </div>
        <div class="timeTableLesson"> Sunday: <input name="Sunday" value="{{studentInfo.Sunday}}" type="time"></div>
      </div>
      <div style="display:flex; justify-content: center;
">
        <button style="width: 100px;">Save</button>
      </div>


    </form>

